{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/App",
            "module": "uvicorn",
            "python": "/usr/local/bin/python",
            "console": "integratedTerminal",
            "args": [
                "app:app",
                "--reload",
                "--host",
                "127.0.0.1",
                "--port",
                "50505"
            ]
        }
    ]
}